@article{crdenasconejo_2020,
title = {An exclusive 42 amino acid signature in pp1ab protein provides insights into the evolutive history of the 2019 novel human-pathogenic coronavirus ({SARS}-{CoV}-2).},
author = {Cárdenas-Conejo, Yair and Liñan-Rico, Andrómeda and García-Rodríguez, Daniel Alejandro and Centeno-Leija, Sara and Serrano-Posada, Hugo},
