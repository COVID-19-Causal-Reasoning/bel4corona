@article{ou_2020,
title = {Characterization of spike glycoprotein of {SARS}-{CoV}-2 on virus entry and its immune cross-reactivity with {SARS}-{CoV}.},
author = {Ou, Xiuyuan and Liu, Yan and Lei, Xiaobo and Li, Pei and Mi, Dan and Ren, Lili and Guo, Li and Guo, Ruixuan and Chen, Ting and Hu, Jiaxin and Xiang, Zichun and Mu, Zhixia and Chen, Xing and Chen, Jieyong and Hu, Keping and Jin, Qi and Wang, Jianwei and Qian, Zhaohui},
