@article{siegel_2011,
title = {Regulation of {IFN}-\lambda1 promoter activity ({IFN}-\lambda1/{IL}-29) in human airway epithelial cells.},
author = {Siegel, Rachael and Eskdale, Joyce and Gallagher, Grant},
pages = {5636-5644},
url = {http://dx.doi.org/10.4049/jimmunol.1003988},
year = {2011},
