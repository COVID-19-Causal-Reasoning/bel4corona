@article{wang_2020a,
title = {A human monoclonal antibody blocking {SARS}-{CoV}-2 infection},
author = {Wang, Chunyan and Li, Wentao and Drabek, Dubravka and Okba, Nisreen M.A. and van Haperen, Rien and Osterhaus, Albert D.M.E. and van Kuppeveld, Frank J.M. and Haagmans, Bart L. and Grosveld, Frank and Bosch, Berend-Jan},
