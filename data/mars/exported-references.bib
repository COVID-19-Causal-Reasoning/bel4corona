@article{bakkers_2017,
title = {Betacoronavirus Adaptation to Humans Involved Progressive Loss of Hemagglutinin-Esterase Lectin Activity.},
author = {Bakkers, Mark J G and Lang, Yifei and Feitsma, Louris J and Hulswit, Ruben J G and de Poot, Stefanie A H and van Vliet, Arno L W and Margine, Irina and de Groot-Mijnes, Jolanda D F and van Kuppeveld, Frank J M and Langereis, Martijn A and Huizinga, Eric G and de Groot, Raoul J},
