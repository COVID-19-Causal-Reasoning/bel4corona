@article{song_2019,
title = {From {SARS} to {MERS}, Thrusting Coronaviruses into the Spotlight.},
author = {Song, Zhiqi and Xu, Yanfeng and Bao, Linlin and Zhang, Ling and Yu, Pin and Qu, Yajin and Zhu, Hua and Zhao, Wenjie and Han, Yunlin and Qin, Chuan},
