@article{andersen_2020,
title = {Discovery and development of safe-in-man broad-spectrum antiviral agents.},
author = {Andersen, Petter I and Ianevski, Aleksandr and Lysvand, Hilde and Vitkauskiene, Astra and Oksenych, Valentyn and Bj\or\aas, Magnar and Telling, Kaidi and Lutsar, Irja and Dumpis, Uga and Irie, Yasuhiko and Tenson, Tanel and Kantele, Anu and Kainov, Denis E},
