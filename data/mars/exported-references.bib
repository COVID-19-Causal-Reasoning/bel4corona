@article{dewit_2020,
title = {Prophylactic and therapeutic remdesivir ({GS}-5734) treatment in the rhesus macaque model of {MERS}-{CoV} infection.},
author = {de Wit, Emmie and Feldmann, Friederike and Cronin, Jacqueline and Jordan, Robert and Okumura, Atsushi and Thomas, Tina and Scott, Dana and Cihlar, Tomas and Feldmann, Heinz},
