@article{totura_2019,
title = {Broad-spectrum coronavirus antiviral drug discovery.},
author = {Totura, Allison L and Bavari, Sina},
pages = {397-412},
url = {http://dx.doi.org/10.1080/17460441.2019.1581171},
year = {2019},
