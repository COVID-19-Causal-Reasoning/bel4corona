@article{dugourd_2020,
title = {Causal integration of multi-omics data with prior knowledge to generate mechanistic hypotheses},
author = {dugourd, Aurelien and Kuppe, Christoph and Sciacovelli, Marco and Gjerga, Enio and Emdal, Kristina Bennet and Bekker-Jensen, Dorte Breinholdt and Kranz, Jennifer and Bindels, Eric J.M. and Costa, Sofia and Olsen, Jesper V and Frezza, Christian and Kramann, Rafael and Saez-Rodriguez, Julio},
